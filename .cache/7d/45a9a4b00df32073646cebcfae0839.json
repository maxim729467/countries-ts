\n.country-item {\n  font-size: 30px;\n  font-weight: 700;\n\n  display: flex;\n  align-items: center;\n}\n\n/* .country-item:focus,\n.country-item:hover {\n  color: rgba(194, 73, 190, 0.8);\n} */\n\n.country-link {\n  font-size: 30px;\n  font-weight: 700;\n  text-decoration: none;\n\n  display: flex;\n  align-items: center;\n  color: inherit;\n\n  transition: all 300ms ease;\n  outline: none;\n}\n\n.country-link:focus,\n.country-link:hover {\n  color: rgba(194, 73, 190, 0.8);\n}\n\n.country-name {\n  margin-left: 15px;\n  margin-top: 0;\n  margin-bottom: 0;\n}\n\n.country-item:not(:last-child) {\n  margin-bottom: 10px;\n}\n\n.placeholder-list {\n  list-style: none;\n  margin: 0;\n  padding: 0;\n}\n\n.placeholder-item {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n.placeholder-item:not(:last-child) {\n  margin-bottom: 40px;\n}\n\n.placeholder-mark {\n  width: 20px;\n  height: 20px;\n  margin-right: 20px;\n  border-radius: 50%;\n  background-color: rgba(180, 144, 202, 0.4);\n}\n\n.placeholder-row {\n  width: 350px;\n  height: 30px;\n  border-radius: 5px;\n  background-color: rgba(180, 144, 202, 0.4);\n}\n\n.loader {\n  border: 16px solid #f3f3f3;\n  border-top: 16px solid #732d94;\n  border-radius: 50%;\n  width: 120px;\n  height: 120px;\n  animation: spin 2s linear infinite;\n\n  position: absolute;\n  top: 40%;\n  left: 47%;\n\n  top: 50;\n  left: 50;\n}\n\n.is-hidden {\n  display: none;\n  pointer-events: none;\n}\n\n@keyframes spin {\n  0% {\n    transform: rotate(0deg);\n  }\n  100% {\n    transform: rotate(360deg);\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"bc897d16cb70c2866d09b600324a6adb","cacheData":{"env":{}}}